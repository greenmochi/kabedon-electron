  "scripts": {
    "start": "cross-env NODE_ENV=dev concurrently \"yarn run react:start\" \"yarn run electron:start\"",
    "build": "yarn run react:build && yarn run electron:build",
    "test": "",
    "react:start": "react-scripts start",
    "react:build": "tsc -p tsconfig.production.json && react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "electron:start": "tsc -p tsconfig.electron.json && electron ./electron_build/main",
    "electron:build": "tsc -p tsconfig.electron.json && electron-builder -c.extraMetadata.main=./electron_build/main",
    "electron:test": ""
  },
  "dependencies": {
    "@types/jest": "24.0.13",
