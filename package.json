    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2",
    "tslint": "^5.16.0"
